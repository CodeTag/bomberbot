<table class="table">
  <thead>
    <tr>
      <th>Posicion</th>
      <th>Usuario</th>
      <th>Puntos</th>
      <th>Partidas</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(function(user, index){ %>
    <tr>
      <td><%= index+1 %></td>
      <td><%= user.get('username') %></td>
      <td><%= user.get('totalPrueba') %></td>
      <td><%= user.get("partidasJugadas") %></td>
    </tr>
    <% }) %>
  </tbody>
</table>